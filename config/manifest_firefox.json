{
  "manifest_version": 2,
  "name": "Epiboard - New Tab",
  "short_name": "Epiboard",
  "description": "A new tab page extension with material design",
  "author": "Alexays",
  "version": "$version",
  "permissions": [
    "storage",
    "sessions",
    "browsingData",
    "downloads",
    "downloads.open",
    "management",
    "tabs",
    "topSites",
    "bookmarks",
    "history",
    "geolocation",
    "https://trends.google.com/trends/hottrends/visualize/internal/data",
    "https://www.google.com/doodles/search"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com/ https://www.gstatic.com/charts/; object-src 'self'",
  "optional_permissions": [
    "https://*/",
    "http://*/"
  ],
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "applications": {
    "gecko": {
      "id": "contact@arouillard.fr",
      "strict_min_version": "54.0"
    }
  }
}
