{
    "manifest_version": 2,
    "name": "epiboard",
    "description": "New tab page",
    "version": "1.0",
    "permissions": [
        "storage",
        "sessions",
        "browsingData",
        "downloads",
        "downloads.open",
        "system.cpu",
        "system.memory",
        "system.storage",
        "https://hawttrends.appspot.com/api/terms/",
        "https://www.google.com/doodles/search"
    ],
    "optional_permissions": [
      "tabs",
      "topSites",
      "bookmarks",
      "management",
      "history"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
